    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@guild-docs/client": "2.1.1",
    "@guild-docs/server": "3.0.1",
    "@mdx-js/react": "^2.1.3",
    "@theguild/components": "^1.12.0",
    "date-fns": "^2.29.2",
    "framer-motion": "^6.5.1",
    "next": "^12.2.5",
    "next-i18next": "^11.3.0",
    "next-seo": "^5.5.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
