    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "@guild-docs/client": "^0.2.28",
    "@guild-docs/server": "^0.2.25",
    "@mdx-js/react": "^1.6.22",
    "@theguild/components": "^1.4.7",
    "date-fns": "^2.23.0",
    "framer-motion": "^4.1.17",
    "next": "^11.1.0",
    "next-i18next": "^8.6.0",
    "next-seo": "^4.26.0",
