    "@guild-docs/server": "^2.1.6",
    "@mdx-js/react": "^1.6.22",
    "@theguild/components": "^1.8.6",
    "date-fns": "^2.28.0",
    "framer-motion": "^5.5.6",
    "next": "^12.0.7",
