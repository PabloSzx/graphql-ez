{
  "name": "graphql-ez",
  "version": "0.10.0",
  "homepage": "https://www.graphql-ez.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/PabloSzx/graphql-ez",
    "directory": "packages/core/main"
  },
  "license": "MIT",
  "author": "PabloSz <pablosaez1995@gmail.com>",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "bob-esbuild watch",
    "prepare": "bob-esbuild build",
    "postpublish": "gh-release",
    "test": "jest"
  },
  "dependencies": {
    "@envelop/core": "^1.0.3",
    "@envelop/disable-introspection": "^1.0.0",
    "@envelop/parser-cache": "^1.0.0",
    "@envelop/types": "^1.0.2",
    "@envelop/validation-cache": "^1.0.0",
    "graphql-helix": "1.7.0"
  },
  "devDependencies": {
    "bob-esbuild-cli": "^0.2.3",
    "changesets-github-release": "0.0.3",
    "eventsource": "^1.1.0",
    "got": "^11.8.2",
    "graphql": "15.4.0-experimental-stream-defer.1",
    "jest": "^27.0.6",
    "ts-jest": "^27.0.4"
  },
  "peerDependencies": {
    "graphql": "*"
  },
  "peerDependenciesMeta": {
    "graphql": {
      "optional": true
    }
  },
  "engines": {
    "node": "^12.20.0 || ^14.13.0 || >=15.0.0"
  },
  "publishConfig": {
    "directory": "dist"
  },
  "typesVersions": {
    "*": {
      "dist/index.d.ts": [
        "dist/index.d.ts"
      ],
      "*": [
        "dist/*",
        "dist/*/index.d.ts"
      ]
    }
  }
}
