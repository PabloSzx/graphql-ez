    "prepack": "bob-esbuild build",
    "postpublish": "gh-release",
    "test": "jest"
  },
  "dependencies": {
    "@graphql-ez/utils": "workspace:^0.1.2",
    "@pablosz/graphql-helix-graphiql": "workspace:^4.2.0"
  },
  "devDependencies": {
    "@types/node": "^16.11.11",
    "bob-esbuild-cli": "^3.0.1",
    "changesets-github-release": "^0.0.4",
    "graphql-ez": "workspace:^0.13.5",
    "jest": "^27.4.3",
    "next": "^12.0.4",
    "ts-jest": "^27.0.7"
  },
  "peerDependencies": {
    "graphql-ez": "workspace:^0.13.5"
  },
  "engines": {
    "node": "^12.20.0 || >=14.13.0"
  },
  "publishConfig": {
    "directory": "dist"
  },
  "typesVersions": {
    "*": {
      "dist/index.d.ts": [
