{
  "name": "@graphql-ez/plugin-graphiql",
  "version": "0.12.1",
  "homepage": "https://www.graphql-ez.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/PabloSzx/graphql-ez",
    "directory": "packages/plugin/graphiql"
  },
  "license": "MIT",
  "author": "PabloSz <pablosaez1995@gmail.com>",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "bob-esbuild watch",
    "prepack": "bob-esbuild build",
    "prepare": "true",
    "postpublish": "gh-release",
    "test": "jest"
  },
  "dependencies": {
    "@graphql-ez/utils": "workspace:^",
    "@pablosz/graphql-helix-graphiql": "workspace:^"
  },
  "devDependencies": {
    "@graphql-ez/plugin-schema": "workspace:^",
    "@types/node": "^22.15.19",
    "bob-esbuild-cli": "5.0.0-alpha-227aebc.0",
    "changesets-github-release": "^0.1.0",
    "esbuild": "^0.25.4",
    "graphql-ez": "workspace:^",
    "next": "^12.3.7",
    "typescript": "^5.8.3"
  },
  "peerDependencies": {
    "graphql-ez": "workspace:^"
  },
  "engines": {
    "node": ">=14.13.1"
  },
  "publishConfig": {
    "directory": "dist",
    "linkDirectory": false
  },
  "typesVersions": {
    "*": {
      "dist/index.d.ts": [
        "dist/index.d.ts"
      ],
      "*": [
        "dist/*",
        "dist/*/index.d.ts"
      ]
    }
  }
}
