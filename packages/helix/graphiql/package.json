{
  "name": "@pablosz/graphql-helix-graphiql",
  "version": "4.1.3",
  "description": "A highly evolved GraphQL HTTP Server ðŸ§¬",
  "repository": {
    "type": "git",
    "url": "https://github.com/PabloSzx/graphql-ez",
    "directory": "packages/helix/graphiql"
  },
  "license": "MIT",
  "author": "Daniel Rearden <danielrearden@gmail.com>",
  "sideEffects": false,
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "concurrently -r \"bob-tsm --cjs browser/build.ts\" \"bob-esbuild build\"",
    "prepublishOnly": "pnpm prepack",
    "postpublish": "gh-release"
  },
  "dependencies": {
    "@graphql-ez/utils": "workspace:^0.1.2"
  },
  "devDependencies": {
    "@graphiql/toolkit": "^0.3.2",
    "@graphql-tools/url-loader": "^7.5.2",
    "@graphql-tools/utils": "^8.5.2",
    "@n1ru4l/graphql-live-query": "^0.9.0",
    "@types/prettier": "^2.4.1",
    "@types/prop-types": "^15.7.4",
    "@types/react": "^17.0.34",
    "@types/react-dom": "^17.0.11",
    "bob-esbuild-cli": "^3.0.0",
    "changesets-github-release": "^0.0.4",
    "concurrently": "^6.3.0",
    "copy-to-clipboard": "^3.3.1",
    "cpy": "^8.1.2",
    "esbuild": "^0.13.12",
    "graphiql": "^1.4.8",
    "graphql": "15.4.0-experimental-stream-defer.1",
    "graphql-ez": "workspace:^0.13.4",
    "graphql-language-service-interface": "^2.8.4",
    "prettier": "^2.4.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "typescript": "^4.4.4"
  },
  "publishConfig": {
    "directory": "dist"
  },
  "buildConfig": {
    "copy": [
      "bundle"
    ]
  },
  "typesVersions": {
    "*": {
      "dist/index.d.ts": [
        "dist/index.d.ts"
      ],
      "*": [
        "dist/*",
        "dist/*/index.d.ts"
      ]
    }
  }
}
