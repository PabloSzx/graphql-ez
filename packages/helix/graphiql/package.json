{
  "name": "@pablosz/graphql-helix-graphiql",
  "version": "4.2.2",
  "description": "A highly evolved GraphQL HTTP Server ðŸ§¬",
  "repository": {
    "type": "git",
    "url": "https://github.com/PabloSzx/graphql-ez",
    "directory": "packages/helix/graphiql"
  },
  "license": "MIT",
  "author": "Daniel Rearden <danielrearden@gmail.com>",
  "sideEffects": false,
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "concurrently -r \"bob-tsm browser/build.ts\" \"bob-esbuild build\"",
    "prepublishOnly": "pnpm prepack",
    "postpublish": "gh-release"
  },
  "dependencies": {
    "@graphql-ez/utils": "workspace:^"
  },
  "devDependencies": {
    "@graphiql/toolkit": "0.4.5",
    "@graphql-tools/url-loader": "7.16.26",
    "@graphql-tools/utils": "^9.1.3",
    "@n1ru4l/graphql-live-query": "^0.9.0",
    "@types/node": "^18.11.15",
    "@types/prettier": "^2.7.1",
    "@types/prop-types": "^15.7.5",
    "@types/react": "^17.0.52",
    "@types/react-dom": "^18.0.9",
    "bob-esbuild-cli": "^4.0.0",
    "changesets-github-release": "^0.1.0",
    "concurrently": "^7.6.0",
    "copy-to-clipboard": "^3.3.3",
    "cpy": "^9.0.1",
    "esbuild": "^0.16.9",
    "graphiql": "^2.4.7",
    "graphql": "16.1.0-experimental-stream-defer.6",
    "graphql-ez": "workspace:^",
    "graphql-language-service-interface": "^2.10.2",
    "graphql-ws": "^5.11.2",
    "prettier": "^2.8.1",
    "prop-types": "^15.8.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "typescript": "^4.9.4"
  },
  "publishConfig": {
    "directory": "dist",
    "linkDirectory": false
  },
  "buildConfig": {
    "copy": [
      "bundle"
    ]
  },
  "typesVersions": {
    "*": {
      "dist/index.d.ts": [
        "dist/index.d.ts"
      ],
      "*": [
        "dist/*",
        "dist/*/index.d.ts"
      ]
    }
  }
}
