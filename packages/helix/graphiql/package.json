{
  "name": "@pablosz/graphql-helix-graphiql",
  "version": "2.0.0",
  "description": "A highly evolved GraphQL HTTP Server ðŸ§¬",
  "repository": {
    "type": "git",
    "url": "https://github.com/PabloSzx/graphql-ez",
    "directory": "packages/helix/graphiql"
  },
  "license": "MIT",
  "author": "Daniel Rearden <danielrearden@gmail.com>",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm prepack",
    "bundle": "pnpm bundle:webpack && pnpm bundle:ts",
    "bundle:ts": "node scripts/build-graphiql.cjs",
    "bundle:webpack": "webpack -c browser/webpack.config.js && cpy browser/css/graphiql.min.css bundle/",
    "prepack": "pnpm bundle && bob-esbuild build",
    "prepare": "bob-esbuild build",
    "postpublish": "gh-release"
  },
  "devDependencies": {
    "@graphql-tools/url-loader": "7.0.8",
    "@graphql-tools/utils": "8.1.0",
    "@n1ru4l/graphql-live-query": "0.7.1",
    "@types/prettier": "2.3.2",
    "@types/prop-types": "15.7.4",
    "@types/react": "17.0.17",
    "@types/react-dom": "17.0.9",
    "@types/terser-webpack-plugin": "5.0.4",
    "bob-esbuild": "^1.2.2",
    "bob-esbuild-cli": "^1.0.1",
    "changesets-github-release": "^0.0.3",
    "concurrently": "^6.2.1",
    "copy-to-clipboard": "^3.3.1",
    "cpy-cli": "^3.1.1",
    "graphiql": "^1.4.2",
    "graphql": "15.4.0-experimental-stream-defer.1",
    "graphql-ez": "workspace:^0.11.0",
    "graphql-language-service-interface": "^2.8.4",
    "prettier": "2.3.2",
    "prop-types": "15.7.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "terser-webpack-plugin": "5.1.4",
    "ts-loader": "9.2.5",
    "typescript": "4.4.1-rc",
    "webpack": "5.50.0",
    "webpack-cli": "4.7.2"
  },
  "publishConfig": {
    "directory": "dist"
  }
}
