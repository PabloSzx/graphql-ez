{
  "name": "@pablosz/graphql-helix",
  "version": "2.0.4",
  "description": "A highly evolved GraphQL HTTP Server ðŸ§¬",
  "license": "MIT",
  "author": "Daniel Rearden <danielrearden@gmail.com>",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/PabloSzx/graphql-ez",
    "directory": "packages/helix/core"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    },
    "./util": {
      "types": "./dist/util/index.d.ts",
      "require": "./dist/util/index.js",
      "import": "./dist/util/index.mjs"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "prepack": "bob-esbuild build"
  },
  "devDependencies": {
    "@types/node": "^22.15.29",
    "bob-esbuild-cli": "5.0.0-alpha-d826df0.0",
    "changesets-github-release": "^0.1.0",
    "esbuild": "^0.25.4",
    "graphql": "16.1.0-experimental-stream-defer.6",
    "typescript": "^5.8.3"
  },
  "publishConfig": {
    "directory": "dist",
    "linkDirectory": false
  },
  "peerDependencies": {
    "graphql": "*"
  }
}
