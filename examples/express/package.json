{
  "name": "example-app-express",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "dev": "yarn dev:mjs",
    "dev:cjs": "tsup-node src/index.ts --format cjs --sourcemap --no-splitting --watch src --onSuccess \"node dist/index.js\"",
    "dev:mjs": "tsup-node src/index.ts --format esm --sourcemap --no-splitting --watch src --onSuccess \"node dist/index.mjs\"",
    "start": "node dist/index.js"
  },
  "dependencies": {
    "@graphql-ez/express": "workspace:^0.1.1",
    "@graphql-typed-document-node/core": "^3.1.0",
    "express": "^4.17.1",
    "graphql": "15.4.0-experimental-stream-defer.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.12",
    "@types/node": "^15.12.2",
    "tsup": "^4.11.2",
    "typescript": "^4.3.2"
  }
}
