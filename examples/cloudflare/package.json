{
  "name": "example-cloudflare",
  "version": "0.0.1",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "esbuild src/index.ts --tsconfig=./tsconfig.json --target=es2020 --minify --bundle --outfile=dist/worker.js && node postbuild.mjs",
    "dev": "concurrently -r \"esbuild src/index.ts --tsconfig=./tsconfig.json --target=es2020 --minify --bundle --outfile=dist/worker.js --watch\" \"miniflare dist/worker.js --watch\"",
    "start": "miniflare dist/worker.js"
  },
  "dependencies": {
    "@graphql-ez/cloudflare": "workspace:*",
    "@graphql-ez/plugin-altair": "workspace:^0.9.9",
    "@graphql-ez/plugin-graphiql": "workspace:^0.11.4",
    "@graphql-ez/plugin-schema": "workspace:^0.8.4",
    "@graphql-ez/plugin-voyager": "workspace:^0.9.3",
    "graphql": "16.1.0-experimental-stream-defer.6",
    "graphql-ez": "workspace:^0.14.1",
    "worktop": "^0.7.3"
  },
  "devDependencies": {
    "@types/node": "^17.0.34",
    "clipboardy": "^3.0.0",
    "concurrently": "^7.2.0",
    "esbuild": "^0.14.39",
    "miniflare": "^2.4.0",
    "typescript": "^4.6.4"
  }
}
