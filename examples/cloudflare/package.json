{
  "name": "example-cloudflare",
  "version": "0.0.1",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "esbuild src/index.ts --tsconfig=./tsconfig.json --target=es2020 --minify --bundle --outfile=dist/worker.js && node postbuild.mjs",
    "dev": "concurrently -r \"esbuild src/index.ts --tsconfig=./tsconfig.json --target=es2020 --minify --bundle --outfile=dist/worker.js --watch\" \"miniflare dist/worker.js --watch\"",
    "start": "miniflare dist/worker.js"
  },
  "dependencies": {
    "@graphql-ez/cloudflare": "workspace:0.2.3",
    "@graphql-ez/plugin-altair": "workspace:^0.10.0",
    "@graphql-ez/plugin-graphiql": "workspace:^0.11.6",
    "@graphql-ez/plugin-schema": "workspace:^0.8.6",
    "@graphql-ez/plugin-voyager": "workspace:^0.9.5",
    "graphql": "16.1.0-experimental-stream-defer.6",
    "graphql-ez": "workspace:^0.15.2",
    "worktop": "^0.7.3"
  },
  "devDependencies": {
    "@types/node": "^18.11.15",
    "clipboardy": "^3.0.0",
    "concurrently": "^7.6.0",
    "esbuild": "^0.16.8",
    "miniflare": "^2.11.0",
    "typescript": "^4.9.4"
  }
}
