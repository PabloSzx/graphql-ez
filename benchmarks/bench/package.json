{
  "name": "benchmark-bench",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "author": "PabloSz <pablosaez1995@gmail.com>",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "bob-esbuild build -i src/schema.ts --bundle --skipTsc",
    "prepack": "bob-esbuild build",
    "results": "bob-tsm --cjs src/results.ts",
    "start": "bob-tsm --node-env=prod src/start.ts",
    "start-ez-fastify": "bob-tsm --node-env=prod src/start-ez-fastify.ts"
  },
  "dependencies": {
    "@graphql-tools/schema": "^8.3.1",
    "autocannon": "^7.5.0",
    "chalk": "^5.0.0",
    "cli-table": "^0.3.11",
    "concurrently": "^6.4.0",
    "cross-env": "^7.0.3",
    "faker": "^5.5.3",
    "kill-port": "^1.6.1",
    "md5": "^2.3.0",
    "mkdirp": "^1.0.4",
    "require-env-variable": "^4.0.0",
    "rimraf": "^3.0.2",
    "systeminformation": "^5.9.17",
    "typescript": "^4.5.4",
    "wait-on": "^6.0.0"
  },
  "devDependencies": {
    "@types/autocannon": "^4.1.1",
    "@types/cli-table": "^0.3.0",
    "@types/concurrently": "^6.4.0",
    "@types/faker": "^5.5.9",
    "@types/md5": "^2.3.1",
    "@types/mkdirp": "^1.0.2",
    "@types/node": "^16.11.14",
    "@types/rimraf": "^3.0.2",
    "@types/wait-on": "^5.3.1",
    "bob-esbuild-cli": "^3.0.1",
    "bob-tsm": "^0.3.7",
    "graphql": "16.0.0-experimental-stream-defer.5"
  },
  "peerDependencies": {
    "graphql": "*"
  }
}
