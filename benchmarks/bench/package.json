{
  "name": "benchmark-bench",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "author": "PabloSz <pablosaez1995@gmail.com>",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "bob-esbuild build -i src/schema.ts --bundle --skipTsc",
    "prepack": "bob-esbuild build",
    "results": "node -r esbuild-register src/results.ts",
    "start": "node -r esbuild-register src/start.ts",
    "start-ez-fastify": "node -r esbuild-register src/start-ez-fastify.ts"
  },
  "dependencies": {
    "@graphql-tools/schema": "^8.2.0",
    "autocannon": "^7.4.0",
    "chalk": "^4.1.2",
    "cli-table": "^0.3.6",
    "concurrently": "^6.2.2",
    "cross-env": "^7.0.3",
    "execa": "^5.1.1",
    "faker": "^5.5.3",
    "graphql": "15.4.0-experimental-stream-defer.1",
    "kill-port": "^1.6.1",
    "md5": "^2.3.0",
    "mkdirp": "^1.0.4",
    "require-env-variable": "^3.1.2",
    "rimraf": "^3.0.2",
    "systeminformation": "^5.9.4",
    "typescript": "^4.4.3",
    "wait-on": "^6.0.0"
  },
  "devDependencies": {
    "@types/autocannon": "^4.1.1",
    "@types/cli-table": "^0.3.0",
    "@types/concurrently": "^6.2.1",
    "@types/faker": "^5.5.8",
    "@types/md5": "^2.3.1",
    "@types/mkdirp": "^1.0.2",
    "@types/node": "^16.10.1",
    "@types/rimraf": "^3.0.2",
    "@types/wait-on": "^5.3.1",
    "bob-esbuild-cli": "^2.0.0",
    "esbuild-register": "^3.0.0"
  }
}
